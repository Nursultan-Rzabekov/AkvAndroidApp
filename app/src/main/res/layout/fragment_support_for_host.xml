<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider45">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/dim_28dp"
            android:paddingEnd="@dimen/dim_28dp"
            android:paddingBottom="@dimen/dim_28dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView1516"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_20dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/thanks_for_choose"
                android:textAlignment="textStart"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content3"
                android:textStyle="bold"
                android:typeface="normal"/>

            <TextView
                android:id="@+id/textView1517"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_12dp"
                android:fontFamily="@font/montserrat"
                android:text="@string/dump_text"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content4"
                android:typeface="normal"/>

            <TextView
                android:id="@+id/textView1518"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_48dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/your_pricate_data"
                android:textAlignment="textStart"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content3"
                android:textStyle="bold"
                android:typeface="normal" />

            <TextView
                android:id="@+id/textView1519"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_12dp"
                android:fontFamily="@font/montserrat"
                android:text="@string/dump_text"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content4"
                android:typeface="normal" />

            <TextView
                android:id="@+id/textView1520"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_48dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/savings"
                android:textAlignment="textStart"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content3"
                android:textStyle="bold"
                android:typeface="normal"/>

            <TextView
                android:id="@+id/textView1521"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_12dp"
                android:fontFamily="@font/montserrat"
                android:text="@string/dump_text"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content4"
                android:typeface="normal"/>

        </LinearLayout>
    </ScrollView>

</LinearLayout>