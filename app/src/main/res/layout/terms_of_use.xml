<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/terms_back_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/BasicBackButton1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lineHeight="20dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/content_margin6"
        android:layout_marginTop="118dp"
        android:layout_marginEnd="@dimen/content_margin7"
        android:fontFamily="@font/montserrat_bold"
        android:text="@string/rules_for_using_app"
        android:textAlignment="textEnd"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_header3"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider3"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="38dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="21dp"
                android:layout_marginStart="@dimen/content_margin8"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/thanks_for_choose"
                android:textAlignment="textStart"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content3"
                android:textStyle="bold"
                android:typeface="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider3" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/content_margin8"
                android:layout_marginTop="13dp"
                android:layout_marginEnd="@dimen/content_margin9"
                android:fontFamily="@font/montserrat"
                android:text="@string/dump_text"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content4"
                android:typeface="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/content_margin8"
                android:layout_marginTop="50dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/your_profile"
                android:textAlignment="textStart"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content3"
                android:textStyle="bold"
                android:typeface="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/content_margin8"
                android:layout_marginTop="13dp"
                android:layout_marginEnd="@dimen/content_margin9"
                android:fontFamily="@font/montserrat"
                android:text="@string/dump_text"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content4"
                android:typeface="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/content_margin8"
                android:layout_marginTop="42dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/services"
                android:textAlignment="textStart"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content3"
                android:textStyle="bold"
                android:typeface="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/content_margin8"
                android:layout_marginTop="13dp"
                android:layout_marginEnd="@dimen/content_margin9"
                android:fontFamily="@font/montserrat"
                android:text="@string/dump_text"
                android:textColor="@android:color/black"
                android:textSize="@dimen/text_content4"
                android:typeface="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />
        </LinearLayout>
    </ScrollView>


    <TextView
        android:id="@+id/akv_label"
        style="@style/AKV_Label"
        android:text="@string/akv"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/content_margin8"
        android:layout_marginEnd="@dimen/content_margin12"
        android:layout_marginBottom="45dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="@string/any_questions"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_content4"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/akv_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>